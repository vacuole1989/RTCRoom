     <live-pusher
            style="width:100%;height:100%;postion：absoulte"
            id="camera-push"
            mode="RTC"
            autopush="true"
            orientation="{{orientation}}"
            beauty="{{beauty}}"
            whiteness="{{whiteness}}"
            background-mute="{{backgroundMute}}"
            url="{{pushUrl}}"
            enable-camera="{{enableCamera}}"
            debug="{{debug}}"
            hidden="{{hide}}"
            bindstatechange="onPushEvent">
                   <cover-view class='bottom_box'>
            <cover-image class="bottom_button gamepad" src="../Resources/wifi.png" bindtap='onGameClick'></cover-image>
        </cover-view>
    </live-pusher> 

