.contain {
    background-color: white;
    width: 100%;
}

.itemList {
    width: 100%;
}

.itemList .item {
    width: 100%;
    height: 100rpx;
    border-top: 1px solid #ccc;
    font-size: 30rpx;
    line-height: 100rpx;
    padding: 0 40rpx;
}

.itemList .item image {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    margin-top: 20rpx;
    right: 40rpx;
}
